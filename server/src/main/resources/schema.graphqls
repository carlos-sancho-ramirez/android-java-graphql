type Query {
  bookById(id: ID): Book
  allPeople: [Person!]!
  allNotes: [Note!]!
}

type Mutation {
  createPerson(email: String!, name: String!): Person
  createNote(title: String!, authorEmail: String!): Note
}

type Book {
  id: ID
  name: String
  pageCount: Int
  author: Author
}

type Author {
  id: ID
  firstName: String
  lastName: String
}

type Person {
  email: String!
  name: String!
}

type Note {
  id: ID!
  title: String!
  body: String
  author: Person!
}