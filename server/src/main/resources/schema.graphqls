type Query {
  bookById(id: ID): Book
  allPeople: [Person!]!
}

type Mutation {
  createPerson(email: String!, name: String!): Person
}

type Book {
  id: ID
  name: String
  pageCount: Int
  author: Author
}

type Author {
  id: ID
  firstName: String
  lastName: String
}

type Person {
  email: String!
  name: String!
}